<launch>

<rosparam file="$(find ROBOT_PRO_MODIFICADO_description)/launch/controller.yaml" command="load"/>
<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="ROBOT_PRO_MODIFICADO" args="joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/ROBOT_PRO_MODIFICADO/joint_states"/>
</node>

</launch>